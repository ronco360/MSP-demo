{
    "class": "Telemetry",
    "My_System": {
        "class": "Telemetry_System",
        "allowSelfSignedCert": true,
        "systemPoller": {
            "interval": 60
        }
    },    
    "My_Consumer": {
        "class": "Telemetry_Consumer",
        "type": "AWS_CloudWatch",
        "region": "{{ aws.region }}",
        "allowSelfSignedCert": true,
        "logGroup": "{{ tags.CWLogGroup }}",
        "logStream": "{{ tags.CWLogStream }}",
        "username": "{{ aws_access_key_id }}",
        "passphrase": {
            "cipherText": "{{ aws_secret_access_key }}"
        }
    },
    "My_Listener": {
        "class": "Telemetry_Listener",
        "port": 6514
    }
}
